android_app {
    name: "DeviceParts",
    defaults: [
        "SettingsLibDefaults",
    ],

    srcs: ["src/**/*.java"],
    resource_dirs: ["res"],
    certificate: "platform",
    platform_apis: true,
    privileged: true,

    static_libs: [
        "androidx.core_core",
        "androidx.preference_preference",
        "//hardware/xiaomi:vendor.xiaomi.hardware.motor-V1.0-java",
    ],

    optimize: {
        proguard_flags_files: ["proguard.flags"],
    },
}
